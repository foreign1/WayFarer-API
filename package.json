{
  "name": "WayFarer-API",
  "version": "1.0.0",
  "description": "A public bus transportation booking server back-end API",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=dev nodemon --exec babel-node ./src/index.js",
    "test-init": "jasmine.js init",
    "test": "NODE_ENV=test babel-node spec/run.js",
    "coveralls-coverage": "coveralls < ./coverage/lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < ./coverage/lcov.info",
    "test:coverage": "NODE_ENV=test nyc jasmine && cat ./coverage/lcov.info && npm run coveralls-coverage && npm run coveralls-coverage && rm -rf ./coverage",
    "lint": "eslint '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foreign/WayFarer-API.git"
  },
  "keywords": [
    "node",
    "rest",
    "api",
    "wayfarer"
  ],
  "author": "John Jenyo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foreign1/WayFarer-API/issues"
  },
  "homepage": "https://github.com/foreign1/WayFarer-API#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "babel-plugin-istanbul": "^5.1.4",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "jasmine": "^3.4.0",
    "jasmine-console-reporter": "^3.1.0",
    "node-mocks-http": "^1.7.6",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
